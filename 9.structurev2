pragma solidity >=0.7.0 <0.9.0; 

struct project {
    uint hasd_id;
    string add;
}
contract demo {

    project public s1;
    project public s2;
    project public s3;
    function deploy (uint a, string memory b) public 
    {
        s1.hasd_id=a;
        s1.add=b;

        s2.hasd_id=a*2;
        s2.add=b;

        s3.hasd_id=a*3;
        s3.add=b;
       
    }
}
